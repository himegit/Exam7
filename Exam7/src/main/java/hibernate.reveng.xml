<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table-filter match-name="customer" match-catalog="sakila"></table-filter>
	<table-filter match-name="address" match-catalog="sakila"></table-filter>
	<table-filter match-name="payment" match-catalog="sakila"></table-filter>
	<table-filter match-name="rental" match-catalog="sakila"></table-filter>
	<table catalog="sakila" name="customer"></table>
	<table catalog="sakila" name="address"></table>
	<table catalog="sakila" name="payment"></table>
	<table catalog="sakila" name="rental"></table>
</hibernate-reverse-engineering>